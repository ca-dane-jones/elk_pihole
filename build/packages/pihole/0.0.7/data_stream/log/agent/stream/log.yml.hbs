paths:
{{#each paths as |path i|}}
  - {{path}}
{{/each}}
exclude_files: [".gz$"]
tags:
{{#if preserve_original_event}}
  - preserve_original_event
{{/if}}
{{#each tags as |tag|}}
  - {{tag}}
{{/each}}
{{#if processors}}
processors:
{{processors}}
{{/if}}
multiline:
    pattern: '^.*:\s(?:forwarded|reply|cached|gravity|config|\/[a-zA-Z0-9_-]+){1}.+$'
    negate: false
    match: after
